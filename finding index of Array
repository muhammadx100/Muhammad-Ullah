import java.util.Scanner;
class ABZ{
  public static void main(String args[]){
   Scanner in= new Scanner(System.in);
   int [] numbers={12, 13, 1, 99, 101};
   int size=5;
   int sum=Integer.MAX_VALUE;
   int index=0;
   for(int i=0; i<size; i++){
    if(numbers[i]<sum){
      sum=numbers[i];
      index=i;
      
    }
   
  }
  System.out.println(sum+" "+index);

} 
}
