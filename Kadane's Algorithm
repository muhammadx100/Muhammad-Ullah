import java.util.Scanner;
class abc{
   public static void main(String args[]){

         int [] array={1,2,-10,4,-5};
         int length=5;
      int max=Integer.MIN_VALUE;
      for(int i=0; i<length; i++){
         int sum=0;
         sum+= array[i];
         max=Math.max(sum, max);
         if(sum<0){
            sum=0;
         }
      }

         System.out.println(max);
   } 
   }
