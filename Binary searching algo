class abc{
    public static void main(String args[]){
     int [] array={1,2,3,4,5};
    int left=0, right=array.length-1;
    int mid=0;
    int target=5;
    while(left<=right){
        mid=left+(right-left)/2;
         if(array[mid]==target){
          System.out.println(array[mid]);
          
         }
         if(array[mid]<target){
          left=mid+1;
         }
         else{
          right=mid-1;
         }
        
      
    }
 
 }
}
